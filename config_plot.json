{
    "name": "Plot", 
    "n_gpu": 1,

    "model_path": "saved/models/Garaham/0415_170953/checkpoint-epoch12.pth",
    "affiche": "saved/models/Garaham/0415_154950/metrics.csv",


    "model": {
        "type": "Model_Mult",
        "args": {
            "num_classes": 2,
            "feature_extract": false,
            "use_pretrained": true,
            "model_name": "alexnet"        
        }
    },

    "data_loader": {
        "type": "odr_data_loader", 
        "args":{
            "data_dir": "data/ODIR-5K/csv/normale_vs_diabetic.csv",
            "image_dir": "data/ODIR-5K/ODIR-5K/preprocess_graham/",
            "batch_size": 512,
            "shuffle": false,
            "equal_dist":true,
            "validation_split": 0.05,
            "num_workers": 2
        }
    },
    "trainer": {
        "epochs": 10,

        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 50,

        "tensorboard": true
    }

}